{
  "name": "parse-vue-spa-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn kill; node script/generateEnv.js; ENV=dev & nodemon server/index.js & vue-cli-service serve",
    "init:key": "bash init.sh",
    "kill": "yarn kill:1337; yarn kill:8081; yarn kill:8082",
    "kill:1337": "kill $(lsof -t -i:1337)",
    "kill:8081": "kill $(lsof -t -i:8081)",
    "kill:8082": "kill $(lsof -t -i:8082)",
    "server:prod": "ENV=prod yarn build; ENV=prod node server/index.js",
    "start": "yarn server:prod",
    "build": "yarn init:key; node script/generateEnv.js; vue-cli-service build",
    "lint": "vue-cli-service lint",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@google-cloud/language": "^3.7.0",
    "@parse/s3-files-adapter": "^1.4.0",
    "@vitalets/google-translate-api": "^3.0.0",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "express": "^4.17.1",
    "parse-dashboard": "^2.0.5",
    "parse-server": "^4.3.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-loading-overlay": "^3.2.0",
    "vue-router": "^3.1.3",
    "vue-stash": "^2.0.1-beta"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chromedriver": "79",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "geckodriver": "^1.19.1",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.2",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "tiptap": "^1.26.6",
    "vue-highlightable-input": "^1.0.9",
    "vue-multiselect": "^2.1.6",
    "vue-template-compiler": "^2.6.10"
  }
}
